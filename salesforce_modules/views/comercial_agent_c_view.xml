<odoo>
    <data>
        <!--Comercial Agent C Layout form view -->
        <record id="comercial_agent_c_form" model="ir.ui.view">
            <field name="name">Comercial Agent C Layout</field>
            <field name="model">model_comercial_agent_c</field>
            <field name="arch" type="xml">
                <form string="Comercial Agent C Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="Name"/>
                                <field name="Agent_Type"/>
                                <field name="create_uid" string="Created By"/>
                            </group>
                            <group>
                                <field name="OwnerId"/>
                                <field name="write_uid" string="Last Modified By"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Open Activities">
                            </page>

                            <page string="Activity History">
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

        <!-- Comercial Agent C Layout tree view -->
        <record id="comercial_agent_c_tree" model="ir.ui.view">
            <field name="name">Comercial Agent C Tree</field>
            <field name="model">model_comercial_agent_c</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Name"/>
                    <field name="Agent_Type"/>
                    <field name="OwnerId"/>
                </tree>
            </field>
        </record>

        <!-- Comercial Agent C Layout action window -->
        <record id="comercial_agent_c_action" model="ir.actions.act_window">
            <field name="name">Comercial Agent C Action Layout</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">model_comercial_agent_c</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('comercial_agent_c_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('comercial_agent_c_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>


    </data>
</odoo>