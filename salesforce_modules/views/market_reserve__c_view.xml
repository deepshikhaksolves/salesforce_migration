<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
         <!--Market Reserve Layout form view -->
        <record id="market_reserve_c_form" model="ir.ui.view">
            <field name="name"> Market Reserve C Layout</field>
            <field name="model">model_market_reserve__c</field>
            <field name="arch" type="xml">
                <form string="Market Reserve C Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="Name"/>
                                <field name="Seller__c"/>
                                <field name="Start__c"/>
                                <field name="How_c"/>
                                <field name="Account__c"/>
                            </group>
                            <group>
                                <field name="OwnerId"/>
                                <field name="Reason__c"/>
                                <field name="End__c"/>

                            </group>
                        </group>
                        <notebook>
                            <page string="Market Reserve History">

                            </page>
                            <page string="Open Activities">

                            </page>
                            <page string="Activity History">

                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
         <!-- Market Reserve C Layout tree view -->
        <record id="market_reserve_c_tree" model="ir.ui.view">
            <field name="name">Market_Reserve__c_Tree</field>
            <field name="model">model_market_reserve__c</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Name" />
                    <field name="Seller__c"/>
                    <field name="Start__c"/>
                    <field name="End__c"/>
                    <field name="OwnerId"/>
                </tree>
            </field>
        </record>

        <record id="market_reserve_c_action" model="ir.actions.act_window">
            <field name="name">Market Reserve C Action Layout</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">model_market_reserve__c</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0), 
                (0, 0, {'view_mode': 'tree', 'view_id': ref('market_reserve_c_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('market_reserve_c_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>
    </data>
</odoo>