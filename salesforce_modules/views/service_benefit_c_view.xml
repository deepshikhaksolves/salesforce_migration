<odoo>
    <data>
        <!--Service Benefit C Layout form view -->
        <record id="service_benefit_c_form" model="ir.ui.view">
            <field name="name">Service Benefit C Layout</field>
            <field name="model">model_service_benefit_c</field>
            <field name="arch" type="xml">
                <form string="Servcie Benefit C Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="Name"/>
                                <field name="Benefits"/>
                            </group>
                            <group>
                                <field name="service_id"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Service Benefit C Layout tree view -->
        <record id="service_benefit_c_tree" model="ir.ui.view">
            <field name="name">Service Benefit C Tree</field>
            <field name="model">model_service_benefit_c</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Name"/>
                     <field name="Benefits"/>
                    <field name="service_id"/>
                </tree>
            </field>
        </record>

        <!-- Service Benefit C Layout action window -->
        <record id="service_benefit_c_action" model="ir.actions.act_window">
            <field name="name">Service Benefit C Action Layout</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">model_service_benefit_c</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('service_benefit_c_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('service_benefit_c_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>
    </data>
</odoo>