<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="contract_service_view_form" model="ir.ui.view">
        <field name="name">contract.service.view.form</field>
        <field name="model">contract.service</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <group>
                            <field name="Service"/>
                            <field name="Service_Start"/>
                            <field name="Service_End"/>
                        </group>
                        <group>
                            <field name="Periodicity"/>
                            <field name="accountable"/>
                            <field name="detailing"/>
                        </group>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="contract_service_action" model="ir.actions.act_window">
        <field name="name">contract.service</field>
        <field name="res_model">contract.service</field>
        <field name="view_mode">form</field>
    </record>
</odoo>
