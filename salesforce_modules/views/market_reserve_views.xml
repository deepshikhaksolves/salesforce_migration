<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
        <!-- market_reserve form view -->
        <record id="market_reserve_form" model="ir.ui.view">
            <field name="name">market_reserve Layout</field>
            <field name="model">market_reserve</field>
            <field name="arch" type="xml">
                <form string="Market Reserve">
                    <sheet>
                        <group>
                            <group>
                                <field name="name" />
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- market_reserve tree view -->
        <record id="market_reserve_layout_tree" model="ir.ui.view">
            <field name="name">market_reserve Layout Tree</field>
            <field name="model">market_reserve</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <!-- market_reserve action window -->
        <record id="market_reserve_action" model="ir.actions.act_window">
            <field name="name">Market Reserve</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">market_reserve</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0), 
                (0, 0, {'view_mode': 'tree', 'view_id': ref('market_reserve_layout_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('market_reserve_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>
    </data>
</odoo>