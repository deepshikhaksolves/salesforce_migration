<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
        <!-- Contract-User Company Contract Permissions Layout form view -->
        <record id="user_contract_layout_form" model="ir.ui.view">
            <field name="name">Contract-User Layout</field>
            <field name="model">user_company_contract_permissions__c</field>
            <field name="arch" type="xml">
                <form string="User-Contract Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="id__c" />
                                <field name="contract__c" />
                                <field name="RecordTypeId"/>
                                <field name="create_uid" />
                            </group>
                            <group>
                                <field name="status__c"/>
                                <field name="Name"/>
                                <field name="OwnerId"/>
                                <field name="write_uid" string='Last Modified By'/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- Contract-User tree view -->
        <record id="user_contract_layout_tree" model="ir.ui.view">
            <field name="name">User Contract Layout Tree</field>
            <field name="model">user_company_contract_permissions__c</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="id__c"/>
                    <field name="contract__c"/>
                    <field name="RecordTypeId"/>
                    <field name="create_uid" />
                    <field name="write_uid"/>
                </tree>
            </field>
        </record>

        <!-- Contract-User action window -->
        <record id="user_contract_layout_action" model="ir.actions.act_window">
            <field name="name">User Contract Layout</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">user_company_contract_permissions__c</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0), 
                (0, 0, {'view_mode': 'tree', 'view_id': ref('user_contract_layout_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('user_contract_layout_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>

