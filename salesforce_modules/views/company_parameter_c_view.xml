<odoo>
    <data>
        <!--Company Parameter C Layout form view -->
        <record id="company_parameter_c_form" model="ir.ui.view">
            <field name="name">Company Parameter C Layout</field>
            <field name="model">model_company_parameter_c</field>
            <field name="arch" type="xml">
                <form string="Company Parameter C Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="Name"/>
                                <field name="study_configuration_id"/>
                                <field name="priority"/>
                                <field name="value"/>
                                <field name="create_uid" string="Created By"/>
                            </group>
                            <group>
                                <field name="company_id"/>
                                <field name="write_uid" string="Last Modified By"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Files">
                            </page>

                            <page string="Notes and Attachments">
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

        <!-- Company Parameter C Layout tree view -->
        <record id="company_parameter_c_tree" model="ir.ui.view">
            <field name="name">Company Parameter C Tree</field>
            <field name="model">model_company_parameter_c</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="Name"/>
                    <field name="study_configuration_id"/>
                    <field name="priority"/>
                    <field name="value"/>
                </tree>
            </field>
        </record>

        <!-- Company Parameter C Layout action window -->
        <record id="company_parameter_c_action" model="ir.actions.act_window">
            <field name="name">Company Parameter C Action Layout</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">model_company_parameter_c</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('company_parameter_c_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('company_parameter_c_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>

    </data>
</odoo>