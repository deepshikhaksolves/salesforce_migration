                                <field name="AccountId"/>
                                <field name="Contract_id"/>
                                <field name="ContactId"/>
                                <field name="Status" required="1"/>
                                <field name="ParentId"/>
                                <field name="IsEscalated"/>
                                <field name="Reopening"/>
                                <field name="SuppliedEmail"/>
                                <field name="Description"/>
                                <field name="SuppliedName"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="Case Comments">
                            </page>

                            <page string="Case Milestones">
                            </page>

                            <page string="Emails">
                            </page>

                            <page string="Attachments">
                                <field name="attachment_ids">
                                    <tree editable="bottom">
                                        <field name="name"/>
                                        <field name="document_use"/>
                                        <field name="OwnerId"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="Notes">
                            </page>

                            <page string="Case Team">
                            </page>

                            <page string="Contact Roles">
                            </page>

                            <page string="Related Cases">
                            </page>

                            <page string="Case History">
                            </page>

                            <page string="Approval History">
                            </page>
                        </notebook>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

