        <!--Case - Risk Management Layout form view -->
        <record id="case_risk_management_form" model="ir.ui.view">
            <field name="name"> Case - Risk Management Layout</field>
            <field name="model">model_case</field>
            <field name="arch" type="xml">
                <form string="Case - Risk Management Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="Subject"/>
                                <field name="Origin" required="1"/>
                                <field name="Type"/>
                                <field name="Reason"/>
                                <field name="Request"/>
                                <field name="Priority"/>
                                <field name="CreatedDate"/>
                                <field name="ClosedDate"/>
                                <field name="IsStopped"/>
                                <field name="Cancelation_Reason"/>
                                <field name="opportunity_id"/>
                                <field name="quote_id"/>
