<?xml version="1.0" encoding="UTF-8"?>
    <odoo>
    <data>
       <record id="opportunity_cr_form" model="ir.ui.view">
            <field name="name">Opportunity Contact Role Layout Form</field>
            <field name="model">crm.lead.contact.role</field>
            <field name="arch" type="xml">
                <form string="Opportunity Contact Role Layout">
                    <sheet>
                        <group>
                            <field name="opportunity_id"/>
                            <field name="contact_id"/>
                            <field name="role"/>
                            <field name="is_primary"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
       <record id="opportunity_cr_tree" model="ir.ui.view">
            <field name="name">Opportunity Contact Role Layout Tree</field>
            <field name="model">crm.lead.contact.role</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="opportunity_id"/>
                    <field name="contact_id"/>
                    <field name="role"/>
                    <field name="is_primary"/>
                </tree>
            </field>
        </record>
       <record id="opportunity_cr_action" model="ir.actions.act_window">
        <field name="name">Opportunity Contact Role</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">crm.lead.contact.role</field>
        <field name="view_mode">tree,form</field>
        <field name="view_ids" eval="[(5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('opportunity_cr_tree')}),
            (0, 0, {'view_mode': 'form', 'view_id': ref('opportunity_cr_form')})]"/>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No records available.
            </p>
        </field>
    </record>

        <record id="opportunity_lineitem_form" model="ir.ui.view">
            <field name="name">Opportunity Product Layout Form</field>
            <field name="model">crm.lead.lineitem</field>
            <field name="arch" type="xml">
                <form string="Opportunity Product Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="opportunity_id"/>
                                <field name="product2Id"/>
                                <field name="unit_price"/>
                                <field name="quantity"/>
                                <field name="carrier"/>
                                <field name="user_id"/>
                                <field name="description"/>
                            </group>
                            <group>
                                <field name="service_date"/>
                                <field name="product_code"/>
                                <field name="list_price"/>
                                <field name="TotalPrice"/>
                                <field name="last_modified_by_id"/>
                            </group>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
