        <record id="opportunity_product_c_form" model="ir.ui.view">
            <field name="name">Opportunity Product Layout Form</field>
            <field name="model">crm.lead.product</field>
            <field name="arch" type="xml">
                <form string="Opportunity Product Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="winner"/>
                                <field name="product"/>
                            </group>
                            <group>
                               <field name="life"/>
                                <field name="consultation_reimbursement"/>
                                <field name="carrier"/>
                            </group>

                        </group>
                        <notebook>
                            <page string="Open Activities" name="open_activities">
                                <group></group>
                            </page>
                            <page string="Activity History" name="activities_history">
                                <group></group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
