<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>
        <!--Acccount Layout form view -->
        <record id="account_layout_form" model="ir.ui.view">
            <field name="name">Account Layout</field>
            <field name="model">account.account</field>
            <field name="arch" type="xml">
                <form string="account Layout">
                    <sheet>
                        <group>
                            <group>
                                <field name="code"/>
                                <field name="user_type_id" string='Account Record Type'/>
                                <field name="ParentId" />
                                <field name="financial_group__c" />
                                <field name="CNPJ__c"/>
                                <field name="Social_Reason__c"/>
                                <field name="name"/>
                                <!-- <field name="Name"/> -->
                                <field name="State_Registration__c"/>
                                <field name="Municipal_Registration__c"/>
                                <field name="CNAE__c" string="NINE"/>
                                <field name="Risk_Level__c"/>
                                <field name="Foundation_Date__c"/>
                                <field name="Account__c" />
                                <field name="Broker__c"/>
                                <field name="Company_Classification__c"/>
                                <field name="Company_Origin__c"/>
                                <field name="create_uid" string='Created By'/>
                                <field name="Description"/>
                            </group>
                            <group>
                                <field name="Start_in_Broker__c"/>
                                <field name="Type"/>
                                <field name="NumberOfEmployees"/>
                                <field name="AnnualRevenue"/>
                                <field name="Website"/>
                                <field name="Email__c"/>
                                <field name="phone"/>
                                <field name="Ownership"/>
                                <field name="Score__c"/>
                                <field name="write_uid"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <!-- model.name tree view -->
        <record id="account_layout_tree" model="ir.ui.view">
            <field name="name">Account Layout Tree</field>
            <field name="model">account.account</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="Account__c"/>
                    <field name="ParentId"/>
                    <field name="create_uid" />
                    <field name="write_uid"/>
                </tree>
            </field>
        </record>

        <!-- account_layout action window -->
        <record id="account_layout_action" model="ir.actions.act_window">
            <field name="name">Account Layout</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">account.account</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0), 
                (0, 0, {'view_mode': 'tree', 'view_id': ref('account_layout_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('account_layout_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>

