<odoo>
    <data>
        <!--Sensus Management Document C Layout form view -->
        <record id="sensus_management_document_c_form" model="ir.ui.view">
            <field name="name">Sensus Management Document C Layout</field>
            <field name="model">model_sensus_management_document_c</field>
            <field name="arch" type="xml">
                <form string="Sensus Management Document C Layout">
                    <sheet>
                        <group>
                            <group>
<!--                                <field name="document_id"/> Document id not define-->
                                <field name="Type_of_Beneficiary"/>
                                <field name="Degree_of_Kinship"/>
                            </group>
                            <group>
                                <field name="Required"/>
                                <field name="Type_of_Movement"/>
                            </group>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>

         <!-- Sensus Management Document C tree view -->
        <record id="sensus_management_document_c_tree" model="ir.ui.view">
            <field name="name">Sensus Management Document C Tree</field>
            <field name="model">model_sensus_management_document_c</field>
            <field name="arch" type="xml">
                <tree>
<!--                    <field name="document_id"/> Document id not define-->
                    <field name="Type_of_Beneficiary"/>
                    <field name="Degree_of_Kinship"/>
                </tree>
            </field>
        </record>

        <!-- Sensus Management Document C Layout action window -->
        <record id="sensus_management_document_c_action" model="ir.actions.act_window">
            <field name="name">Sensus Management Document C Action Layout</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">model_sensus_management_document_c</field>
            <field name="view_mode">tree,form</field>
            <field name="view_ids" eval="[(5, 0, 0),
                (0, 0, {'view_mode': 'tree', 'view_id': ref('sensus_management_document_c_tree')}),
                (0, 0, {'view_mode': 'form', 'view_id': ref('sensus_management_document_c_form')})]"/>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    No records available.
                </p>
            </field>
        </record>

    </data>
</odoo>