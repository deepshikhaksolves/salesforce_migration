<?xml version="1.0" encoding="UTF-8"?>
    <odoo>
    <data>
       <record id="financial_doctor_form" model="ir.ui.view">
            <field name="name">Financial Doctor Layout Form</field>
            <field name="model">financial_doctor</field>
            <field name="arch" type="xml">
                <form string="Financial Doctor">
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="email_id"/>
                                <field name="active"/>
                                <field name="financial_group_id"/>
                                <field name="create_uid"/>
                            </group>
                            <group>
                                <field name="OwnerId"/>
                                <field name="write_uid" string="Last Modified By"/>
                            </group>
                        </group>
                    </sheet>
                    <div class="oe_chatter">
                        <field name="message_follower_ids" widget="mail_followers"/>
                        <field name="activity_ids" widget="mail_activity"/>
                        <field name="message_ids" widget="mail_thread"/>
                    </div>
                </form>
            </field>
        </record>
       <record id="financial_doctor_tree" model="ir.ui.view">
            <field name="name">Financial Doctor Layout Tree</field>
            <field name="model">financial_doctor</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="email_id"/>
                    <field name="active"/>
                    <field name="financial_group_id"/>
                    <field name="create_uid"/>
                </tree>
            </field>
        </record>
       <record id="financial_doctor_action" model="ir.actions.act_window">
        <field name="name">Financial Doctor</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">financial_doctor</field>
        <field name="view_mode">form</field>
        <field name="view_ids" eval="[(5, 0, 0),
            (0, 0, {'view_mode': 'tree', 'view_id': ref('financial_doctor_tree')}),
            (0, 0, {'view_mode': 'form', 'view_id': ref('financial_doctor_form')})]"/>
        <field name="help" type="html">
            <p class="o_view_nocontent_smiling_face">
                No records available.
            </p>
        </field>
    </record>
    </data>
</odoo>